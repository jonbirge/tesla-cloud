{
    "name": "jonbirge/tesla-cloud",
    "description": "Tesla Cloud - In-car browser web application",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "sendinblue/api-v3-sdk": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "TeslaCloud\\": "php/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
